def int main<>
{
  int x = 10;
  int y = 3;
  
  print<"Variable x:">;
  print<x>;
  print<"Variable y:">;
  print<y>;
  print<"Addition (x + y):"
  print<x + y>;
  print<"Subtraction (x - y):">;
  print<x - y>;
  print<"Multiplication (x * y):">;
  print<x * y>;
  print<"Division (x / y), should be an integer:">;
  print<x / y>;
  print<"Modulo (x % y):"
  print<x % y>;

  return 0;
}
