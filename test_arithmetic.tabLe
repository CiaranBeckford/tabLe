def int main<>
{
  int x;
  int y;

  x=10;
  y=3;

  prints<"Variable x:">;
  print<x>;
  prints<"Variable y:">;
  print<y>;
  prints<"Addition (x + y):">;
  print<x + y>;
  prints<"Subtraction (x - y):">;
  print<x - y>;
  prints<"Multiplication (x * y):">;
  print<x * y>;
  prints<"Division (x / y), should be an integer:">;
  print<x / y>;
  prints<"Modulo (x % y):">;
  print<x % y>;

  return 0;
}
