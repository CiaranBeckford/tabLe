def int main<>
{
  int x = 10.0;
  int y = 3.0;
  
  prints<"Variable x:">;
  printf<x>;
  prints<"Variable y:">;
  printf<y>;
  prints<"Addition (x + y):"
  printf<x + y>;
  prints<"Subtraction (x - y):">;
  printf<x - y>;
  prints<"Multiplication (x * y):">;
  printf<x * y>;
  prints<"Division (x / y):">;
  printf<x / y>;
  prints<"Modulo (x % y):"
  printf<x % y>;

  return 0;
}
